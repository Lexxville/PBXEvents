<xsd:schema
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:tns="http://3cx.pbxevents.jjinterna.com/cdrsocket/model"
    targetNamespace="http://3cx.pbxevents.jjinterna.com/cdrsocket/model">

    <xsd:element name="CallDetail">
        <xsd:complexType>
            <xsd:sequence>
                <xsd:element name="detailNum" type="xsd:int" />
                <xsd:element name="idCallHistory2" type="xsd:string" />
                <xsd:element name="parentDetailNum" type="xsd:int" />
                <xsd:element name="destDn" type="xsd:string" />
                <xsd:element name="destNum" type="xsd:string" />
                <xsd:element name="startTime" type="xsd:string" />
                <xsd:element name="duration" type="xsd:string" />
                <xsd:element name="answerTime" type="xsd:string" />
                <xsd:element name="status" type="xsd:string" />
                <xsd:element name="isCompl" type="xsd:int" />
                <xsd:element name="specDestType" type="xsd:string" />
                <xsd:element name="isToOutside" type="xsd:int" />
            </xsd:sequence>
        </xsd:complexType>
    </xsd:element>      

	<xsd:element name="Call">
    	<xsd:complexType>
   			<xsd:sequence>
   				<xsd:element name="callId" type="xsd:string" />
   				<xsd:element name="duration" type="xsd:string" />
	   			<xsd:element name="startTime" type="xsd:string" />
   				<xsd:element name="answerTime" type="xsd:string" />
   				<xsd:element name="endTime" type="xsd:string" />
   				<xsd:element name="from_no" type="xsd:string" />
   				<xsd:element name="to_no" type="xsd:string" />
	   			<xsd:element name="group_no" type="xsd:string" />
   				<xsd:element name="line_no" type="xsd:string" />
   				<xsd:element name="is_answ" type="xsd:int" />
   				<xsd:element name="is_fail" type="xsd:int" />
	   			<xsd:element name="is_compl" type="xsd:int" />
   				<xsd:element name="is_fromoutside" type="xsd:int" />
   				<xsd:element name="callerId" type="xsd:string" />
   				<xsd:element name="dialedNumber" type="xsd:string" />
	   			<xsd:element name="lastCallerId" type="xsd:string" />
   				<xsd:element name="lastDialedNumber" type="xsd:string" />   			
   				<xsd:element name="mediaType" type="xsd:int" />
   				<xsd:element name="rate" type="xsd:int" />
	   			<xsd:element name="totalCost" type="xsd:int" />
   				<xsd:element name="billPrefix" type="xsd:string" />
   				<xsd:element name="billRateName" type="xsd:string" />
                <xsd:element name="numDetails" type="xsd:int" />
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>		

</xsd:schema>