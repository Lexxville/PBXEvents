        			<xsd:element name="agentAddress" type="xsd:string" />        	
				</xsd:sequence>
        	</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

    <xsd:complexType name="PhoneLineDiscovered">
        <xsd:complexContent>
        	<xsd:extension base="tns:PBXEvent">
        		<xsd:sequence>
        			<xsd:element name="lineNumber" type="xsd:string" />
        			<xsd:element name="phoneAddress" type="xsd:string" />
        			<xsd:element name="port" type="xsd:int" />
				</xsd:sequence>
        	</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

    <xsd:complexType name="PBXQueueEvent">
        <xsd:complexContent>
        	<xsd:extension base="tns:PBXEvent">
        		<xsd:sequence>
					<xsd:element name="timeId" type="xsd:long" />
