        	</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

    <xsd:complexType name="PhoneDiscovered">
        <xsd:complexContent>
        	<xsd:extension base="tns:PBXEvent">
        		<xsd:sequence>
        			<xsd:element name="sysName" type="xsd:string" />        	
        			<xsd:element name="sysDesc" type="xsd:string" />
